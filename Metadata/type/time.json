{
    "DATE": {
      "byte": 3,
      "range": ["1000-01-01", "9999-12-31"],
      "format": "YYYY-MM-DD",
      "help": "日期值"
    },
    "TIME": {
      "byte": 3,
      "range": ["-838:59:59", "838:59:59"],
      "format": "HH:MM:SS",
      "help": "时间值或持续时间"
    },
    "YEAR": {
      "byte": 1,
      "range": [1901, 2155],
      "format": "YYYY",
      "help": "年份值"
    },
    "DATETIME": {
      "byte": 8,
      "range": ["1000-01-01 00:00:00", "9999-12-31 23:59:59"],
      "format": "YYYY-MM-DD HH:MM:SS",
      "help": "混合日期和时间值"
    },
    "TIMESTAMP": {
      "byte": 4,
      "range": ["1970-01-01 00:00:00", "2038-1-19 03:14:07"],
      "format": "YYYYMMDD HHMMSS",
      "help": "混合日期和时间值，时间戳"
    }
  }